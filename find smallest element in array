#include <stdio.h>

int main()
{
    
    int a[]={1,2,4,5,7};
    int b[]={5,6,3,4,8};
    
    int n=sizeof(a)/sizeof(a[0]);
    int m=sizeof(b)/sizeof(b[0]);
    for(int i=0;i<n;i++)
    {
     for(int j=0;j<m;j++)
     {
         if(a[i]+b[j]==9)
         {
            printf("%d,%d",a[i],b[j]);
            printf("\n");
         }
     }
    }
}
