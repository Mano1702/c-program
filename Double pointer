#include<stdio.h>
int main()
{
int a=10;
int *ptr=&a;
int **dptr=&ptr;
printf("value of a %d\n",a);
printf("address of a %d\n",a);
printf("ptr pointing to the address %d \n",ptr);
printf("dptr pointing to the address %d\n",dptr);
printf("ptr value %d\n",*ptr);
printf("dptr value %d\n",**dptr);
return 0;
}
