scanf("%d",&n);
   int a[n];
int b,c;
printf("enter the number");
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
c=a[n/2];
printf("the mid value is :%d",c);
b=c/4;
if(c%4==0)
{
printf("\n%d",b);
}
else 
{
printf("\nit is impossible");
}
    
