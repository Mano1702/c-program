#include <stdio.h>
int main()
{
    int r,c,i,j,a[10][10];
    printf("enter the no of rows and column\n");
    scanf("%d%d",&r,&c);
    for(i=0;i<r;i++)
    {
        for(j=0;j<c;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    printf("given matrix\n");
    for(i=0;i<r;i++)
    {
        for(j=0;j<c;j++)
        {
            printf("%d  ",a[i][j]);
        }
        printf("\n");
    }
        printf("transpose matrix\n");

for(i=0;i<r;i++)
    {
        for(j=0;j<c;j++)
        {
            printf("%d  ",a[j][i]);
        }
                printf("\n");

    }


    return 0;
}
